{
    "name": "javiereguiluz/symfony-installer",
    "description": "Symfony installer.",
    "keywords": ["symfony", "php", "installer"],
    "license": "MIT",
    "authors": [
        {
            "name": "Symfony Community"
        }
    ],
    "autoload": {
        "psr-4": { "Symfony\\": "src/" }
